/* pages/myClass/myClass.wxss */
page{
  height: 1000px;
}
.container{
  height: 1000px;
  display: flex;
  align-items: center;
  flex-direction: column;
  box-sizing: border-box;
  background-color: #B0C4DE;
}

.list-display{
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  padding: 0;
  box-sizing: border-box;
  border:0;
  
}

.list-classinfo{
  width: 650rpx;
  height: 100rpx;
  padding: 15rpx;
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
  margin-top: 20rpx;
  background-color: white;
  color: #999999;
  border-radius: 0rpx;
  align-items: center;
  justify-content: center;
  font-size: 35rpx;
  border:0;
}


.name{
  padding-left: 50rpx;
  width: 375rpx;
}